	<queryString><![CDATA[SELECT "Code", "ItemName", "คลัง", "ต้นทุน", "Quantity" 
FROM 
(
    SELECT item.item_id, item.item_code "Code", common_name AS "ItemName", stock.stock_name "คลัง", 
        CAST(item.unit_price_cost AS FLOAT) "ต้นทุน", COALESCE(SUM(CAST(stock_mgnt.cur_quantity AS FLOAT)), 0) "Quantity" 
    FROM item 
    LEFT JOIN stock_mgnt ON item.item_id=stock_mgnt.item_id 
    LEFT JOIN stock ON stock_mgnt.stock_id=stock.stock_id 
    WHERE item.active='1' 
    GROUP BY item.item_id, "Code", "ItemName", "คลัง", "ต้นทุน" 
) tmp
WHERE 
tmp."Quantity" < 0
ORDER BY "Code", "ItemName", "คลัง"]]></queryString>
