<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

  <servlet>
    <servlet-name>ListDoctorServlet</servlet-name>
    <display-name>ListDoctorServlet</display-name>
    <servlet-class>com.bpk.servlet.ListDoctorServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>ListAllergyTempForCorrectServlet</servlet-name>
    <display-name>ListAllergyTempForCorrectServlet</display-name>
    <servlet-class>com.bpk.servlet.ListAllergyTempForCorrectServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>Login</servlet-name>
    <display-name>Login</display-name>
    <servlet-class>com.as400samplecode.Login</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>ListDoctorServlet</servlet-name>
    <url-pattern>/ListDoctorServlet</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>ListAllergyTempForCorrectServlet</servlet-name>
    <url-pattern>/ListAllergyTempForCorrectServlet</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/Login</url-pattern>
  </servlet-mapping>

</web-app>